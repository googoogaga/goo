(use proto)
(use proto/eval)
(use proto/system)
(use ast)
(use top)
(use p2c)
;; (use beatbox)
;; (use p2m)
;; (use typist)
;; (use optimize)
;; (use ast-print)

;; Make sure the proto/user module gets compiled into our image.
(use proto/user)

(dv *proto-version* "0.109")
(dv *proto-date*    "OCT 30, 2001")

(format out ";;; \n")
(format out ";;; PROTO\n")
(format out ";;; JONATHAN BACHRACH\n")
(format out ";;; MIT AI Lab\n")
(format out ";;; %s\n" *proto-date*)
(format out ";;; \n")
(format out ";;; VERSION = %s\n" *proto-version*)
(format out ";;; ROOT    = %s\n" *proto-root*)
(format out ";;; \n")
(format out ";;; Loading Runtime Bindings...\n")
(init-ast)
(format out ";;; Loading System Patches...\n")
(load (add-src-path "system-patches.proto") 'proto/user)
(format out ";;; Loading User Patches...\n")
(load (add-src-path "user-patches.proto") 'proto/user)
(format out ";;; \n")
(top 'proto/user)
