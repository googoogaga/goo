(use goo)
(use goo/eval)
(use goo/system)
(use compiler/ast)
(use compiler/top)
(use compiler/g2c)
;; (use compiler/decision-trees)
;; (use tests)
;; (use beatbox)
;; (use compiler/g2m)
;; (use compiler/typist)
;; (use compiler/optimize)
;; (use compiler/ast-print)

;; Make sure the goo/user module gets compiled into our image.
(use goo/user)

(dv *goo-version* "0.114")
(dv *goo-date*    "MAR 14, 2002")

(format out ";;; GOO\n")
(format out ";;; JONATHAN BACHRACH\n")
(format out ";;; MIT AI Lab\n")
(format out ";;; %s\n" *goo-date*)
(format out ";;; \n")
(format out ";;; VERSION = %s\n" *goo-version*)
(format out ";;; ROOT    = %s\n" *goo-root*)
;; (format out ";;; Loading Runtime Bindings...\n")
(init-ast)
;; (format out ";;; Loading System Patches...\n")
(load (add-src-path "system-patches.goo") 'goo/user)
;; (format out ";;; Loading User Patches...\n")
(load (add-src-path "user-patches.goo") 'goo/user)
;; (format out ";;; \n")
(top 'goo/user)
