;; Make sure the proto/user module gets compiled into our image.
(use proto/user)

(dv *goo-version* "0.112")
(dv *goo-date*    "MAR 13, 2002")

(format out ";;; GOO\n")
(format out ";;; JONATHAN BACHRACH\n")
(format out ";;; MIT AI Lab\n")
(format out ";;; %s\n" *goo-date*)
(format out ";;; \n")
(format out ";;; VERSION = %s\n" *goo-version*)
(format out ";;; ROOT    = %s\n" *goo-root*)
;; (format out ";;; Loading Runtime Bindings...\n")
(init-ast)
;; (format out ";;; Loading System Patches...\n")
(load (add-src-path "system-patches.proto") 'proto/user)
;; (format out ";;; Loading User Patches...\n")
(load (add-src-path "user-patches.proto") 'proto/user)
;; (format out ";;; \n")
(top 'proto/user)
