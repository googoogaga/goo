;; Make sure the goo/user module gets compiled into our image.
(use goo/user)

(dv *goo-version* "0.120")
(dv *goo-date*    "MAR 24, 2002")

(msg out ";;; GOO\n")
(msg out ";;; JONATHAN BACHRACH\n")
(msg out ";;; MIT AI Lab\n")
(msg out ";;; %s\n" *goo-date*)
(msg out ";;; \n")
