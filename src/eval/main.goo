(init-ast)
;; (msg out ";;; Loading System Patches...\n")
(load (add-src-path "system-patches.goo") 'goo/user)
;; (msg out ";;; Loading User Patches...\n")
(load (add-src-path "user-patches.goo") 'goo/user)
;; (msg out ";;; \n")
(top 'goo/user)
