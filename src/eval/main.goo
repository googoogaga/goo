(use boot)
(use macros)
(use runtime)
(use write)
(use ast)
(use top)
(use p2c)

(dv *proto-version* "0.97")
(dv *proto-date*    "AUG 01, 2001")

(format out ";;; \n")
(format out ";;; PROTO\n")
(format out ";;; JONATHAN BACHRACH\n")
(format out ";;; MIT AI Lab\n")
(format out ";;; %s\n" *proto-date*)
(format out ";;; \n")
(format out ";;; VERSION = %s\n" *proto-version*)
(format out ";;; ROOT    = %s\n" *proto-root*)
(format out ";;; \n")
(format out ";;; Loading Runtime Bindings...\n")
(init-ast)
(format out ";;; Loading System Patches...\n")
(load (add-src-path "system-patches.proto"))
(format out ";;; Loading User Patches...\n")
(load (add-src-path "user-patches.proto"))
(format out ";;; \n")
(top)
