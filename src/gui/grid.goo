(use goo)
(use goo/boot)

(dl %grid-open)
(dl %grid-close)
(dl %grid-move)
(dl %grid-write)
(dl %grid-refresh)

(df grid-open () (%grid-open))
(df grid-close () (%grid-close))
(df grid-move (x y) (%grid-move (%iu x) (%iu y)))
(df grid-read () (%cb (%grid-read)))
(df grid-write (c) (%grid-write (%cu c)))
(df grid-refresh () (%grid-refresh))

